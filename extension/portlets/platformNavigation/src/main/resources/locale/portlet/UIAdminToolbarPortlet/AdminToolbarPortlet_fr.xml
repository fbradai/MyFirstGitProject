<bundle>
   <UIAdminToolbarPortlet>
		<action>
			<AddPage>Ajouter une Page</AddPage>
			<EditNavigation>Navigation</EditNavigation>
			<Content>Contenu</Content>
			<Editor>Editer</Editor>
			<Page>Page</Page>
			<Site>Site</Site>
			<Layout>Mise en page</Layout>
			<ManagePages>Gestion des pages</ManagePages>
		 	<GroupSite>Site de groupe</GroupSite>
			<Properties>SEO</Properties>
      <AddSite>Ajouter un site</AddSite>
		</action>
		<ManageSites>Gestion des sites</ManageSites>
		<ManageGroupSites>Gestion des sites des groupes</ManageGroupSites>
   </UIAdminToolbarPortlet>
  
  <UISEOForm>
    <label>
      <title>Titre: </title> 
      <description>Description: </description>
      <keywords>Mots clés: </keywords>
      <language>Langue: </language>
      <robots>Robots: </robots>
      <sitemap>Sitemap: </sitemap>
      <sitemapvisible>Visible dans le sitemap</sitemapvisible>
      <priority>Priorité: </priority>
      <frequency>Fréquence: </frequency>
      <select-language>Sélectionnez une langue</select-language>  
      <option>
        <Always>Toujours</Always>
        <Hourly>Toutes les heures</Hourly>
        <Daily>Journalier</Daily>
        <Weekly>Hebdomadaire</Weekly>
        <Monthly>Mensuel</Monthly>
        <Yearly>Annuel</Yearly>
        <Never>Jamais</Never>
      </option> 
    </label>    	  
    <action>
      <Save>Sauvegarder</Save>
      <Cancel>Annuler</Cancel>
    </action>
    <msg>
      <repository-exception>Une erreure inattendue est apparue! Sauvegarde impossible des métadonnées..</repository-exception>
      <confirm-delete>Êtes-vous sûr de vouloir supprimer les données SEO?</confirm-delete>	
    </msg>
    <title>
    	<seo-delete>Supprimer</seo-delete>
    </title>
	<help> 
	  <priority>Valeurs valides de 0.0 à 1.0.</priority>
	  <description>Aide les utilisateurs a savoir en quoi cette page a un lien avec ce qu'ils cherchent.</description>
	  <keywords>Termes les plus importants de la page séparés par une virgule.</keywords>		  
    </help>	 	  
  </UISEOForm>
  <UISEOPopupWindow>
  	<title>
      <UISEOForm>SEO Management</UISEOForm>
  	</title>
  </UISEOPopupWindow>    
  <UISEOToolbarForm>
  	<title>
      <seo>SEO Management</seo>
  	</title>
  </UISEOToolbarForm>  
  <FloatNumberValidator>
    <msg>
      <Invalid-number>Le champ '{0}' est un nombre invalide (float).</Invalid-number>
    </msg>
  </FloatNumberValidator>
  
  <Locale>
    <label>      
      <it>Italien</it>      
      <fr>Français</fr>
      <en>Anglais</en>      
    </label>
  </Locale>
  
